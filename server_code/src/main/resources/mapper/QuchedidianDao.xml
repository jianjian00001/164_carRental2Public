<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.cl.dao.QuchedidianDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.cl.entity.QuchedidianEntity" id="quchedidianMap">
        <result property="didian" column="didian"/>
        <result property="yidihaichedian" column="yidihaichedian"/>
    </resultMap>

    <select id="selectListView"
		resultType="com.cl.entity.view.QuchedidianView" >

		SELECT  quchedidian.* FROM quchedidian  quchedidian 	        
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectView"
		resultType="com.cl.entity.view.QuchedidianView" >
		SELECT * FROM quchedidian  quchedidian <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	

</mapper>
